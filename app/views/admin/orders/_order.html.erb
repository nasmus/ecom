<div id="<%= dom_id order %>" class="w-full sm:w-auto my-5 space-y-5">
  

  <tr class="bg-white border-b  border-gray-200 hover:bg-gray-50">
    <th scope="row" class="px-6 py-4">
      <%= order.customer_email %>      
    </th>
    <th scope="row" class="px-6 py-4">
      <%= order.fulfilled? ? "Yes" : "No" %>
    </th>
    <td class="px-6 py-4">
      <%= order.total %>
    </td>
    <td class="px-6 py-4">
      <%= order.address %>
    </td>
    <td class="px-6 py-4 ">
      <%= link_to "Show", [:admin, order], class: "font-medium text-blue-600 hover:underline" %>
    </td>
    <td class="px-6 py-4 ">
      <%= link_to "Edit", edit_admin_order_path(order), class: "font-medium text-blue-600 hover:underline" %>
    </td>
    <td class="px-6 py-4 ">
      <%= button_to "Destroy", admin_order_path(order), method: :delete, class: "font-medium text-red-600 hover:underline", data: { turbo_confirm: "Are you sure?" } %>
    </td>
  </tr>
  

</div>
